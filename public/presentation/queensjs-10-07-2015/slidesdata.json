    "custom":"<div style='float:left; width:65%'><div class='code markdown' style='margin-bottom:20px;'>Cast On: CO 5</div><div class='code'>var myScarf = [];<br>var row0 = new Array(5);<br><br>// myScarf is <br>// [ , , , , ]</div></div><img src='./img/ks1.jpg'width='30%' style='margin-left:20px;'>"

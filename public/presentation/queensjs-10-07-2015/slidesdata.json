    "custom":"<div style='float:left; width:65%'><div class='code markdown' style='margin-bottom:20px;'>Row 1: k5</div><div class='code javascript'>var myScarf = [];<br>var k = 'K';<br>var row0 = [k,k,k,k,k];<br><br>myScarf.push(row0);<br>// [['K','K','K','K','K']]</div></div><img src='./img/ks2.jpg'width='30%' style='margin-left:20px;'>"
  },
  "layout" :{
    "center":false,
